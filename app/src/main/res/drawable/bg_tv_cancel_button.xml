<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid
                android:color="#E76E6E"/>
            <stroke
                android:width="0.1dp"
                android:color="#A64343"/>

            <size
                android:width="10dp"
                android:height="10dp"
                />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid
                android:color="#EB4646"/>
            <stroke
                android:width="0.1dp"
                android:color="#A64343"/>

            <size
                android:width="10dp"
                android:height="10dp"
                />
        </shape>
    </item>
</selector>
